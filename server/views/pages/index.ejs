<!-- views/index.ejs -->
<!doctype html>
<html>
<head>

	<% include ../partials/header %>

</head>
<body>
	<div id="wrapper">

		<div id="page-wrapper">
			<div class="row">
				<div class="col-lg-12">
					<h1 class="page-header">아이디를 선택해 주십시오.</h1>

					<div class="container col-lg-3">
						<table class="table table-bordered table-hover table-striped">
							<thead>
								<tr>
									<th>ID</th>
								</tr>
							</thead>
							<tbody>
								<% if(IDs) IDs.forEach(function(ID) { %>
								<tr>
									<td>
										<a href='/record?id=<%= ID %>'><%= ID %></a>
									</td>
								</tr>
								<% }); %>
							</tbody>
						</table>



					</div>
				</div>
				<!-- /.col-lg-12 -->
			</div>


		</div>
		<!-- /#page-wrapper -->

	</div>
	<!-- /#wrapper -->

	<% include ../partials/footer %>
</body>
</html>